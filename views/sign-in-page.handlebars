          {{!-- Sign in --}}
          <form class="form" id="signin-form">
          <div class="input-with-label">
            <div class="input">
              <div class="content">
                <input 
                
                  class="password poppins-medium-jaguar-16px"
                  id="password-signin"
                  name="password"
                  placeholder="Password"
                  type="password"
                  required
                />
              </div>
            </div>
          </div>
          <div class="input-with-label-1 input-with-label-6">
            <div class="input">
              <div class="content">
                <input 
                  class="text poppins-medium-jaguar-16px"
                  id="email-signin"
                  name="text"
                  placeholder="Email address"
                  type="email"
                  required
                />
              </div>
            </div>
            <div class="frame-5">
              <a href="javascript:SubmitForm('form2')">
                <div class="button-primary-with-icon-1">
                  <div class="frame-1"><button class="button-name valign-text-middle" type="submit" id="signin-form">Sign in</button></div>
                </div></a
              >
            </div>
          </div>
          </form>


          {{!-- Sign up --}}
          <form class="form" id="signup-form">
          <div class="input-with-label-2 input-with-label-6">
            <div class="input">
              <div class="content">
                <input
                  class="text-1 poppins-medium-jaguar-16px"
                  id="license-signup"
                  name="text"
                  placeholder="License Number"
                  type="number"
                  required
                />
              </div>
            </div>
          </div>
          <div class="input-with-label-3 input-with-label-6">
            <div class="input">
              <div class="content">
                <input
                  class="text-2 poppins-medium-jaguar-16px"
